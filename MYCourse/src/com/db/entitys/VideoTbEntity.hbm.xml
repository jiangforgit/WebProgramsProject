<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.VideoTbEntity" table="video_tb" schema="my_course">
        <id name="videoId" column="VideoId">
            <generator class="increment"></generator>
        </id>
        <property name="videoDescribe" column="VideoDescribe"/>
        <property name="videoMaxOnlineNum" column="VideoMaxOnlineNum"/>
        <property name="videoType" column="VideoType"/>
        <property name="videoCreateTime" column="VideoCreateTime"/>
        <property name="videoUpdateTime" column="VideoUpdateTime"/>
    </class>
</hibernate-mapping>