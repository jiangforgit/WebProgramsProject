<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.LessonTbEntity" table="lesson_tb" schema="my_course">
        <id name="lessonId" column="LessonId">
            <generator class="increment"></generator>
        </id>
        <property name="lessonDescribe" column="LessonDescribe"/>
        <property name="lessonCourseId" column="LessonCourseId"/>
        <property name="lessonClassRoomId" column="LessonClassRoomId"/>
        <property name="lessonType" column="LessonType"/>
        <property name="lessonWeekCourseId" column="LessonWeekCourseId"/>
        <property name="lessonCreateTime" column="LessonCreateTime"/>
        <property name="lessonUpdateTime" column="LessonUpdateTime"/>
    </class>
</hibernate-mapping>