<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.MsgstatusTbEntity" table="msgstatus_tb" schema="my_course">
        <id name="msgStatusId" column="MsgStatusId">
            <generator class="increment"></generator>
        </id>
        <property name="msgStatus" column="MsgStatus"/>
        <property name="msgStatusMId" column="MsgStatusMId"/>
        <property name="msgStatusCreatorType" column="MsgStatusCreatorType"/>
        <property name="msgStatusCreatorId" column="MsgStatusCreatorId"/>
        <property name="msgStatusCreateTime" column="MsgStatusCreateTime"/>
        <property name="msgStatusUpdateTime" column="MsgStatusUpdateTime"/>
    </class>
</hibernate-mapping>