<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.CourseTbEntity" table="course_tb" schema="my_course">
        <id name="courseId" column="CourseId">
            <generator class="increment"></generator>
        </id>
        <property name="courseName" column="CourseName"/>
        <property name="courseDescribe" column="CourseDescribe"/>
        <property name="courseGeneralView" column="CourseGeneralView"/>
        <property name="courseSchoolId" column="CourseSchoolId"/>
        <property name="courseCreateTime" column="CourseCreateTime"/>
        <property name="courseUpdateTime" column="CourseUpdateTime"/>
    </class>
</hibernate-mapping>