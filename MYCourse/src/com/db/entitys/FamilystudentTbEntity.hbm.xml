<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.FamilystudentTbEntity" table="familystudent_tb" schema="my_course">
        <composite-id>
            <key-property name="familyStudentFId" column="FamilyStudentFId"/>
            <key-property name="familyStudentSId" column="FamilyStudentSId"/>
        </composite-id>
        <property name="familyStudentRelateType" column="FamilyStudentRelateType"/>
        <property name="familyStudentCreateTime" column="FamilyStudentCreateTime"/>
        <property name="familyStudentUpdateTime" column="FamilyStudentUpdateTime"/>
    </class>
</hibernate-mapping>