<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.FamilyuserTbEntity" table="familyuser_tb" schema="my_course">
        <id name="familyUserId" column="FamilyUserId">
            <generator class="increment"></generator>
        </id>
        <property name="familyUserName" column="FamilyUserName"/>
        <property name="familyUserSex" column="FamilyUserSex"/>
        <property name="familyUserPhone" column="FamilyUserPhone"/>
        <property name="familyUserCreateTime" column="FamilyUserCreateTime"/>
        <property name="familyUserUpdateTime" column="FamilyUserUpdateTime"/>
    </class>
</hibernate-mapping>