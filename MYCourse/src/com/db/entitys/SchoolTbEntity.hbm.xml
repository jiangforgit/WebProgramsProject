<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.SchoolTbEntity" table="school_tb" schema="my_course">
        <id name="schoolId" column="SchoolId">
            <generator class="increment"></generator>
        </id>
        <property name="schoolName" column="SchoolName"/>
        <property name="schoolAddr" column="SchoolAddr"/>
        <property name="schoolType" column="SchoolType"/>
        <property name="schoolCreatorType" column="SchoolCreatorType"/>
        <property name="schoolCreatorUserId" column="SchoolCreatorUserId"/>
        <property name="schoolCreateTime" column="SchoolCreateTime"/>
        <property name="schoolUpdateTime" column="SchoolUpdateTime"/>
    </class>
</hibernate-mapping>