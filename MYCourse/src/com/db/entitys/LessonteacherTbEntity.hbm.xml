<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.LessonteacherTbEntity" table="lessonteacher_tb" schema="my_course">
        <id name="lessonTeacherId" column="LessonTeacherId">
            <generator class="increment"></generator>
        </id>
        <property name="lessonTeacherType" column="LessonTeacherType"/>
        <property name="lessonTeacherName" column="LessonTeacherName"/>
        <property name="lessonTeacherDes" column="LessonTeacherDes"/>
        <property name="lessonTeacherTId" column="LessonTeacherTId"/>
        <property name="lessonTeacherRoomId" column="LessonTeacherRoomId"/>
        <property name="lessonTeacherCreateTime" column="LessonTeacherCreateTime"/>
        <property name="lessonTeacherUpdateTime" column="LessonTeacherUpdateTime"/>
    </class>
</hibernate-mapping>