<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.db.entitys.UserAuthsEntity" table="user_auths" schema="user_center">
        <id name="id">
            <column name="id" sql-type="varchar(50)" length="50"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="identityType">
            <column name="identity_type" sql-type="int(11)"/>
        </property>
        <property name="identifier">
            <column name="identifier" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="credential">
            <column name="credential" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>